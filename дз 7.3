#include <iostream>

int main() {

    
    int arr[] = { 500, 4, 24, 15, 100, 105 };
    int num;
    int count = 0;

    std::cout << "Number: ";
    std::cin >> num;

    for (int i = 0; i < 6; i++) {
        if (arr[i] % num == 0) {
            count++;
        }
    }

    std::cout << "answer: " << num  << count << std::endl;

    return 0;
}
